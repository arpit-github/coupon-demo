{
  "name": "coupon-demo",
  "description": "",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "dev-vm": "webpack-dev-server --open --mode development --host localhost",
    "build": "webpack --progress -p",
    "server": "node server.js",
    "lint": "eslint --fix --ext .js --ext .jsx src test",
    "test": "jest",
    "storybook": "start-storybook",
    "deploy": "npm run build && npm run server",
    "release": "standard-version",
    "release-dry-run": "standard-version --dry-run"
  },
  "husky": {
    "hooks": {
      "commit-msg": "if git-branch-is master; then commitlint -E HUSKY_GIT_PARAMS; fi"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "commitUrlFormat": "https://github.com/arpit-github/coupon-demo/commits/{{hash}}",
    "compareUrlFormat": "https://github.com/arpit-github/coupon-demo/compare/{{currentTag}}%0D{{previousTag}}"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "Firefox ESR"
  ],
  "engines": {
    "node": "^12.0",
    "npm": "^6.0"
  },
  "dependencies": {
    "antd": "^3.20.7",
    "axios": "^0.18.1",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "promise-polyfill": "^8.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.8.6",
    "react-modal": "^3.9.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^7.6.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/theming": "^5.1.11",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "file-loader": "^3.0.1",
    "git-branch-is": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^6.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:arpit-github/coupon-demo.git"
  },
  "author": "Arpit Gupta",
  "homepage": "",
  "license": "ISC",
  "private": true
}
